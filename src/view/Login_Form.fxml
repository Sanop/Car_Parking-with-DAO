      <JFXComboBox fx:id="cmbUserRole" labelFloat="true" layoutX="21.0" layoutY="168.0" prefHeight="26.0" prefWidth="240.0" promptText="User Role" />
      <ImageView fitHeight="126.0" fitWidth="150.0" layoutX="75.0" layoutY="39.0" pickOnBounds="true" preserveRatio="true">
