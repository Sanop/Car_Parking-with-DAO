      <JFXTextField fx:id="txtUserName" focusColor="#756805" labelFloat="true" layoutX="21.0" layoutY="213.0" prefHeight="26.0" prefWidth="240.0" promptText="User Name" />
      <JFXPasswordField fx:id="txtPassword" focusColor="#756805" labelFloat="true" layoutX="21.0" layoutY="260.0" prefHeight="26.0" prefWidth="240.0" promptText="Password" />
      <Label layoutX="75.0" prefHeight="39.0" prefWidth="133.0" text="Parking_P">
